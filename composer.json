{
  "name": "flipboxdigital/salesforce",
  "description": "Salesforce API.",
  "version": "3.0.0",
  "keywords": [
    "flipbox",
    "salesforce"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "flipboxdigital/relay-salesforce": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "psr/log": "^1.0.1",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Salesforce\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Salesforce\\Tests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "1.0.x-dev"
    }
  }
}